<area shape="poly" title=" " alt="" coords="182,16,214,16,214,21,182,21"/>
<area shape="rect" href="namespacecpplintdocs#af27826b49df1e5e9c87705ff0c469082" title=" " alt="" coords="451,5,581,32"/>
